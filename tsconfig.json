{
    "compilerOptions": {
        "allowSyntheticDefaultImports": true,
        "baseUrl": "",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "moduleResolution": "node",
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "paths": {
            "@data/*": [
                "data/*"
            ],
            "@app/*": [
                "src/*"
            ]
        },
        "resolveJsonModule": true,
        "sourceMap": true,
        "target": "esnext"
    }
}
